{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Gloam.Core/Gloam.Core.csproj",
            "src/Gloam.Core.Ui/Gloam.Core.Ui.csproj",
            "src/Gloam.Data/Gloam.Data.csproj",
            "src/Gloam.Runtime/Gloam.Runtime.csproj",
            "src/Gloam.Console.Render/Gloam.Console.Render.csproj",
            "src/Gloam.Demo/Gloam.Demo.csproj",
            "tools/Gloam.Cli/Gloam.Cli.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "*.md",
          "**/*.md",
          "toc.yml"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "styles/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Gloam Engine Documentation",
      "_appName": "Gloam",
      "_appLogoPath": "images/logo.png",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/tgiachi/gloam",
        "branch": "main"
      },
      "_gitUrlPattern": "github",
      "_appFooter": "Â© 2025 Gloam Contributors. Built with DocFX.",
      "_enablePrint": true,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableNavbar": false,
      "_disableAffix": false,
      "_hideEmptyNamespace": true,
      "_disableLangKeyword": true
    }
  }
}